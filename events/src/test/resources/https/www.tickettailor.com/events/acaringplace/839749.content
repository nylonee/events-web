<style type="text/css">
    html body.showing-modal * { display: none; }
    html body.showing-modal .overlay_wrapper { display: block; }
    html body.showing-modal .overlay_wrapper * { display: inherit !important; }
</style>
<script type="text/javascript">
    $(document).ready(function() {
        var isMobile = (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent));
        $('a.trigger_overlay').click(function (event) {
            if(!isMobile) {
                event.preventDefault();
                loadTicketModal($(this).attr('href'));
            }
        });

        if(window.location.hash == "#ticket_selection") {
            var href = $('a#buy_tickets_button').attr('href');
            if(!isMobile) {
                loadTicketModal(href);
            } else {
                location.href = href;
            }
        }
    });

    function loadTicketModal(url) {

        $('.overlay_wrapper').show();
        $('body').css('overflow', 'hidden');
        loadUrl(url);
        $('body').css('max-height', '100%');
        setTabIndex('.event_page_footer', '-1');
        setTabIndex('.footer_compensation', '-1');
    }

    function loadUrl(url) {
        var overlayWindow = $('#overlay_window');
        if(overlayWindow.length) {
            overlayWindow.attr('src',url);
        } else {
            setTimeout("loadUrl('"+url+"');", 200);
        }
    }

    function closeOverlay(confirmClose) {
        if(!confirmClose || confirm("Are you sure you wish to close this window?")) {
            $('body').removeClass('showing-modal');
            $('.overlay_wrapper').hide();
            $('body').css('overflow', 'initial');
            $('#overlay_window').attr('src', "");
            $('body').css('max-height', 'auto');
            setTabIndex('.event_page_footer', '0');
            setTabIndex('.footer_compensation', '0');
        }
    }

    function setTabIndex(parentElement, tabIndex) {
        $(parentElement).find('a, input, select, button, textarea, iframe').attr('tabindex',tabIndex);
    }
</script>
