<script type="text/javascript">
    $( document ).ready(function() {
        $('a.date_select_button').click(function(e){
            e.preventDefault();
            if($(this).parent().hasClass('open')) {
                $('#datepicker_wrapper').slideUp();
                $('.filter_by_date').removeClass('open');
                $(this).attr('aria-expanded','false');
                $(this).text($(this).text().replace("Hide","Show"));
            } else {
                $('#datepicker_wrapper').slideDown();
                $('.filter_by_date').addClass('open');
                $(this).attr('aria-expanded','true');
                $(this).text($(this).text().replace("Show","Hide"));
            }
        });

        $('.prev_month').click(function(){
            $('#datepicker .ui-datepicker-prev').trigger("click");
        });
        $('.next_month').click(function(){
            $('#datepicker .ui-datepicker-next').trigger("click");
        });

        $('.clear_selection').click(function(){
            showAllDates();
        });

        $('#datepicker').datepicker({
            numberOfMonths: 2,
            minDate: Object.keys(selectableDates)[0], // start the calendar on the first selectable date
            firstDay: 1,
            beforeShowDay: function(date) {
                var mysqlDate = date.toMysqlFormat();
                if(mysqlDate in selectableDates) {
                    if(selectableDates[mysqlDate].available) {
                        return [true, "", selectableDates[mysqlDate].availability_note];
                    }
                    if(selectableDates[mysqlDate].sold_out) {
                        return [true, "sold_out", selectableDates[mysqlDate].unavailable_label];
                    }
                    return [true, "unavailable", selectableDates[mysqlDate].unavailable_label];
                }
                return [false, "", ""];
            },
            onSelect: function(date) {
                showDate(date, $('#fullDateFormat').val());
            },
            dateFormat: "yy-mm-dd",
            altFormat: "D d M yy",
            altField: "#fullDateFormat"
        });

        // no date selected by default
        $('#datepicker').datepicker("setDate", -1);
        $('.ui-datepicker-current-day .ui-state-active').removeClass('ui-state-active');

        $('#datepicker').on('mouseenter', ".ui-datepicker-calendar", function() {
            $(this).tooltip();
        });

        $('p.back_to_top a').click(function(){
            scrollToTop();
        });
    });

    function twoDigits(d) {
        if(0 <= d && d < 10) return "0" + d.toString();
        if(-10 < d && d < 0) return "-0" + (-1*d).toString();
        return d.toString();
    }

    Date.prototype.toMysqlFormat = function() {
        var mysqdlDate = new Date(this.getTime() - this.getTimezoneOffset()*60000);
        return mysqdlDate.getUTCFullYear() + "-" + twoDigits(1 + mysqdlDate.getUTCMonth()) + "-" + twoDigits(mysqdlDate.getUTCDate());
    };

</script>

