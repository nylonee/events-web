        }
    });

    function loadTicketModal(url) {

        $('.overlay_wrapper').show();
        $('body').css('overflow', 'hidden');
        loadUrl(url);
        $('body').css('max-height', '100%');
        setTabIndex('.event_page_footer', '-1');
        setTabIndex('.footer_compensation', '-1');
    }

    function loadUrl(url) {
        var overlayWindow = $('#overlay_window');
        if(overlayWindow.length) {
            overlayWindow.attr('src',url);
        } else {
            setTimeout("loadUrl('"+url+"');", 200);
        }
    }

    function closeOverlay(confirmClose) {
        if(!confirmClose || confirm("Are you sure you wish to close this window?")) {
            $('body').removeClass('showing-modal');
            $('.overlay_wrapper').hide();
            $('body').css('overflow', 'initial');
            $('#overlay_window').attr('src', "");
            $('body').css('max-height', 'auto');
            setTabIndex('.event_page_footer', '0');
            setTabIndex('.footer_compensation', '0');
        }
    }

    function setTabIndex(parentElement, tabIndex) {
